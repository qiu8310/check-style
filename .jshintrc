{
  "es3": false,
  "es5": false,
  "esnext": true,

  "node": true,
  "browser": true,
  "mocha": true,

  "laxbreak": true,
  "bitwise": true,
  "camelcase": false,
  "curly": false,
  "eqeqeq": true,
  "eqnull": true,
  "immed": true,
  "latedef": "nofunc",
  "newcap": true,
  "noarg": true,
  "quotmark": "single",
  "undef": true,
  "unused": "vars",
  "strict": false,
  "trailing": true,
  "smarttabs": true,
  "proto": false,
  "maxlen": 120,
  "globals": {
    "__DEBUG__"  : false,
    "__PROD__"   : false,
    "__DEV__"    : false
  }
}
